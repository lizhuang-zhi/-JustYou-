/* 相册头部 */
.Ablum-head{
  width: 100%;
  height: 200px;
  border-bottom: 1px solid rgb(228, 224, 224);
  background-image:url('https://s1.ax1x.com/2020/10/24/BVQuVS.jpg');
  background-position: 0px 0px;
  background-size: 100% auto;
}
/* 我的相册 */
.Ablum-head .Album-name{
  width: 100%;
  height: 100px;
  line-height: 100px;
  font-size: 25px;
  text-align: center;
}
