.picker{
  width: 100%;
  height: 60px;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.picker .Cancel,.picker .title,.picker .Determine{
  height: 40px;
  line-height: 40px;
  font-size: 16px;
}
.picker .Determine{
  color: rgb(107,124,161);
}
.picker .Cancel{
  color: rgb(150,151,153);
}