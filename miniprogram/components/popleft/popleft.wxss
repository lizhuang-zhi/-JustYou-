.outbox{
  margin-top: 150px;
  padding: 0 18px;
}
.user-list{
  width: 100%;
  height: 40px;
  display: flex;
  justify-content: left;
  text-align: center;
}
.user-list .user-img{
  width: 40px;
  height: 40px;
}
.user-list image{
  border-radius: 50%;
  width: 100%;
  height:100%;
}
.user-list .user-name{
  height:40px;
  font-size: 15px;
  line-height: 40px;
}
.user-name text{
  margin-left: 5px;
}
.list{
  padding:15px 0;
}
.Edit{
  width:100%;
  display: flex;
  text-align: center;
  justify-content: space-between;

}
.Edit text{
  height: 40px;
  line-height: 40px;
  font-size: 15px;
}